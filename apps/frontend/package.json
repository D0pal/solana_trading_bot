		"vite": "^5.0.3",
		"vitest": "^2.0.0",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.7.2",
		"flowbite": "^2.4.1",
		"flowbite-svelte": "^0.46.15",
		"lodash": "^4.17.21",
		"lucide-svelte": "^0.424.0",
		"shared-types": "^1.0.0",
		"svelte-french-toast": "^1.2.0"
