      "moment": "^2.30.1",
      "passport": "^0.7.0",
      "passport-custom": "^1.1.1",
      "passport-jwt": "^4.0.1",
      "passport-local": "^1.0.0",
      "postgres": "^3.4.4",
      "promise-retry": "^2.0.1",
